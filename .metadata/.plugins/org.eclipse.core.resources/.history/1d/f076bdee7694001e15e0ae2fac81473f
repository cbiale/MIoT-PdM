#include "MEF_sistema.h"

void sistema_iniciar() {
	// inicia todo el sistema I2c, lcd, uart
	  I2C_iniciar();
	  lcd_iniciar();
	  uartInit();
	  // activa luz de fondo del lcd
	  lcd_luz_fondo(true);
}

