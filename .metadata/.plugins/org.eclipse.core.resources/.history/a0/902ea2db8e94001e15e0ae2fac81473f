#include <API_sistema.h>
#



void sistema_iniciar() {
	// inicia el sistema: I2c, lcd, uart, interfaz
	I2C_iniciar();
	lcd_iniciar();
	uart_iniciar();
	interfaz_iniciar();
}

void sistema_actualizar() {
	interfaz_actualizar();
}

